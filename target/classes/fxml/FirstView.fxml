      </ImageView>
      <ImageView fx:id="smashBotImageView" fitHeight="93.0" fitWidth="123.0" layoutX="-123.0" layoutY="234.0" onMouseClicked="#SmashRoboterPressed" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../assets/smashbotMove1.gif" />
         </image>
      </ImageView>
      <ImageView fx:id="hulkBotImageView" fitHeight="93.0" fitWidth="148.0" layoutX="-148.0" layoutY="307.0" onMouseClicked="#HulkRoboterPressed" pickOnBounds="true">
